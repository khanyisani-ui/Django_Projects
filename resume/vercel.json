{
    "builds": [
        {
            "src": "resume/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "staticfiles/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/lib/jquery/jquery.min.js",
            "dest": "/static/lib/jquery/jquery.min.js"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/resume/wsgi.py"
        }
    ]
}